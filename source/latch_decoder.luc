module latch_decoder (
    input in[5],
    output out[8][3]
    
  ) {
  
  always {
    case(in){
      1: out = {3b000,3b000,3b000,3b000,3b000,3b000,3b000,3b000}; 
      2: out = {3b000,3b001,3b000,3b000,3b000,3b000,3b000,3b000};
      3: out = {3b000,3b001,3b010,3b000,3b000,3b000,3b000,3b000};
      4: out = {3b000,3b001,3b010,3b011,3b000,3b000,3b000,3b000};
      5: out = {3b000,3b001,3b010,3b011,3b100,3b000,3b000,3b000}; 
      6: out = {3b000,3b001,3b010,3b011,3b100,3b101,3b000,3b000};
      7: out = {3b000,3b001,3b010,3b011,3b100,3b101,3b110,3b000};
      8: out = {3b000,3b001,3b010,3b011,3b100,3b101,3b110,3b111};
      default: out = {3b000,3b000,3b000,3b000,3b000,3b000,3b000,3b000};
      }
  }
}
